<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.tamal.deviceinformation">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SensorActivity"
            android:label="@string/activity_sensor" />
        <activity
            android:name=".SensorDetailActivity"
            android:label="@string/activity_sensor_details"
            android:parentActivityName=".SensorActivity" />
        <activity
            android:name=".ShowDataHelperActivity"
            android:label="@string/activity_show_data_helper" />
    </application>

</manifest>